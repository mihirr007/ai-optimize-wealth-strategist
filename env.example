# =============================================================================
# LLM API Keys
# =============================================================================

# For running LLMs hosted by openai (gpt-4o, gpt-4o-mini, etc.)
# Get your OpenAI API key from https://platform.openai.com/
OPENAI_API_KEY=your-openai-api-key

# For running LLMs hosted by anthropic (claude-3-5-sonnet, claude-3-opus, claude-3-5-haiku)
# Get your Anthropic API key from https://anthropic.com/
ANTHROPIC_API_KEY=your-anthropic-api-key

# For running LLMs hosted by deepseek (deepseek-chat, deepseek-reasoner, etc.)
# Get your DeepSeek API key from https://deepseek.com/
DEEPSEEK_API_KEY=your-deepseek-api-key

# For running LLMs hosted by groq (deepseek, llama3, etc.)
# Get your Groq API key from https://groq.com/
GROQ_API_KEY=your-groq-api-key

# For running LLMs hosted by gemini (gemini-2.5-flash, gemini-2.5-pro)
# Get your Google API key from https://ai.dev/
GOOGLE_API_KEY=your-google-api-key

# =============================================================================
# Market Data APIs - YFinance Agent (FREE)
# =============================================================================
# YFinance is free but has rate limits
YAHOO_FINANCE_API_KEY=your-yahoo-finance-api-key

# =============================================================================
# Market Data APIs - Polygon.io Agent
# =============================================================================
# Real-time quotes for U.S. equities, forex, crypto, plus economic data
# Get from: https://polygon.io/
POLYGON_API_KEY=your-polygon-api-key

# =============================================================================
# Market Data APIs - Alpha Vantage Agent
# =============================================================================
# Technical indicators, fundamentals, forex; supports Canadian stocks
# Get from: https://www.alphavantage.co/
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-api-key

# =============================================================================
# Market Data APIs - Financial Modeling Prep Agent
# =============================================================================
# P&L, cash flow, enterprise value, earnings data
# Get from: https://financialmodelingprep.com/
FMP_API_KEY=your-fmp-api-key

# =============================================================================
# Market Data APIs - Twelve Data Agent
# =============================================================================
# Real-time and historical price data, global coverage, WebSocket support
# Get from: https://twelvedata.com/
TWELVE_DATA_API_KEY=your-twelve-data-api-key

# =============================================================================
# Market Data APIs - Marketstack Agent
# =============================================================================
# Global equity prices (delayed), works well for lightweight tasks
# Get from: https://marketstack.com/
MARKETSTACK_API_KEY=your-marketstack-api-key


# =============================================================================
# News & Sentiment APIs - NewsAPI US Agent
# =============================================================================
# U.S. business and market news headlines
# Get from: https://newsapi.org/
NEWSAPI_US_KEY=your-newsapi-us-key

# =============================================================================
# News & Sentiment APIs - NewsAPI Canada Agent
# =============================================================================
# Canada-specific business and policy news
NEWSAPI_CANADA_KEY=your-newsapi-canada-key

# =============================================================================
# News & Sentiment APIs - Finnhub News Agent
# =============================================================================
# Real-time financial news headlines, sentiment analysis, press releases
# Get from: https://finnhub.io/
FINNHUB_API_KEY=your-finnhub-api-key


# =============================================================================
# Macro & Economic Data APIs - FRED Agent
# =============================================================================
# U.S. macro indicators like CPI, interest rates, GDP
# Get from: https://fred.stlouisfed.org/
FRED_API_KEY=your-fred-api-key

# =============================================================================
# Macro & Economic Data APIs - Polygon.io Economic Agent
# =============================================================================
# Inflation, interest rates, economic reports (U.S. focus)
# Uses same POLYGON_API_KEY as above

# =============================================================================
# System Configuration
# =============================================================================
DEFAULT_MODEL=gpt-4o
DEFAULT_PROVIDER=OpenAI
OLLAMA_BASE_URL=http://localhost:11434
DATABASE_URL=sqlite:///wealth_management.db
LOG_LEVEL=INFO

# =============================================================================
# Market Data Configuration
# =============================================================================
# Cache duration in seconds (5 minutes default)
MARKET_DATA_CACHE_DURATION=300

# Rate limiting (requests per minute)
YFINANCE_RATE_LIMIT=60
POLYGON_RATE_LIMIT=30
ALPHA_VANTAGE_RATE_LIMIT=20
FMP_RATE_LIMIT=40
TWELVE_DATA_RATE_LIMIT=50
MARKETSTACK_RATE_LIMIT=30

# News API rate limits
NEWSAPI_RATE_LIMIT=100
FINNHUB_RATE_LIMIT=60
FOREX_NEWS_RATE_LIMIT=30
FXSTREET_RATE_LIMIT=40

# Economic data rate limits
FRED_RATE_LIMIT=120 